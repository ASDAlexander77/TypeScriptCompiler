{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) tsc.exe - DUMP",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../../__build/tsc/bin/tsc.exe",
            "args": ["-emit=ast", "C:/temp/1.ts"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal",
            "visualizerFile": "${workspaceFolder}/../tsc.natvis"
        },
        {
            "name": "(Windows) tsc.exe - MLIR",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../../__build/tsc/bin/tsc.exe",
            "args": ["-emit=mlir", "-debug-only=mlir", "C:/temp/1.ts"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal",
            "visualizerFile": "${workspaceFolder}/../tsc.natvis"
        },
        {
            "name": "(Windows) tsc.exe - MLIR-AFFINE",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../../__build/tsc/bin/tsc.exe",
            "args": ["-emit=mlir-affine", "C:/temp/1.ts"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal",
            "visualizerFile": "${workspaceFolder}/../tsc.natvis"
        },
        {
            "name": "(Windows) tsc.exe - MLIR-LLVM",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../../__build/tsc/bin/tsc.exe",
            "args": ["-emit=mlir-llvm", "C:/temp/1.ts"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal",
            "visualizerFile": "${workspaceFolder}/../tsc.natvis"
        },
        {
            "name": "(Windows) tsc.exe - LLVM",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../../__build/tsc/bin/tsc.exe",
            "args": ["-emit=llvm", "C:/temp/1.ts"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal",
            "visualizerFile": "${workspaceFolder}/../tsc.natvis"
        },
        {
            "name": "(Windows) tsc.exe - JIT",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../../__build/tsc/bin/tsc.exe",
            "args": ["-emit=jit", "C:/temp/1.ts"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal",
            "visualizerFile": "${workspaceFolder}/../tsc.natvis"
        },        
        {
            "name": "(Windows) tsc.exe - JIT-OBJ",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../../__build/tsc/bin/tsc.exe",
            "args": ["-emit=jit", "-dump-object-file", "-object-filename=out.o", "C:/temp/1.ts"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal",
            "visualizerFile": "${workspaceFolder}/../tsc.natvis"
        },        
        {
            "name": "(Windows) CMake current - Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": ["-emit=mlir", "C:/temp/1.ts"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal"
        }
    ]
}
