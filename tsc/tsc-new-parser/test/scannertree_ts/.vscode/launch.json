    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch TS-Node - scannertree",
            "runtimeExecutable": "ts-node",
            "program": "${workspaceFolder}/scannertree.ts",
            "cwd": "${workspaceFolder}",
            "args": [
                "G:/Dev/TypeScript/tests/cases/compiler/error___.ts"
            ],
            "sourceMaps": true,
            "outFiles": [
              "${workspaceRoot}/*.js"
            ],
            "protocol": "inspector"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch TS-Node - parsertree",
            "runtimeExecutable": "ts-node",
            "program": "${workspaceFolder}/parsertree.ts",
            "cwd": "${workspaceFolder}",
            "args": [
                "G:/Dev/TypeScript/tests/cases/compiler/error___.ts"
            ],
            "sourceMaps": true,
            "outFiles": [
              "${workspaceRoot}/*.js"
            ],
            "protocol": "inspector"
        },        
        {
            "name": "Launch Program - match",
            "program": "${workspaceFolder}/match.mjs",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch TS-Node - match",
            "runtimeExecutable": "ts-node",
            "program": "${workspaceFolder}/match.ts",
            "cwd": "${workspaceFolder}",
            "args": [
            ],
            "sourceMaps": true,
            "outFiles": [
            ],
            "protocol": "inspector"
        },        
    ]
}