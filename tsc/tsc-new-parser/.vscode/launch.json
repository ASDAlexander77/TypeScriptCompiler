            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "visualizerFile": "${workspaceFolder}/../tsc.natvis"
        },        
        {
            "name": "(Windows) CMake current - Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": ["-emit=mlir", "C:/temp/1.ts"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
